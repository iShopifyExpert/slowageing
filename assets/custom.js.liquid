    getVersion: function() {
        var ua = window.navigator.userAgent;
        var msie = ua.indexOf('MSIE ');
        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\.?/)) {
            if (!!navigator.userAgent.match(/Trident.*rv\:11\.?/))
                return 11;
            return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)));
        } else {
            return false;
        }
        return false;
    }
}
