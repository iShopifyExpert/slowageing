.main-slider.slick-dotted.slick-slider{margin-bottom:40px;}
.slideshow_image-mob{display:none;}
.main-slider_slide .slideshow_image{width:100%;min-height:280px;vertical-align:top;}
.slideshow-container{max-width: 100%;padding:0 0px;margin:0 auto;position:absolute;left: 0; right: 0;top:50px;text-align:left;-webkit-transform: none;-moz-transform: none;-ms-transform: none;-o-transform: none;transform: none;}
.slideshow_text-content{width:50%;padding:0 30px;opacity:1;}
.slideshow_heading{font-size:7rem;max-width:100%;line-height:1;margin-bottom: 20px;padding-bottom:30px;position:relative;opacity:0;}
.slideshow_heading:after{content:'';width:0%;height:3px;background-color:#f2a900;position:absolute;left:0;bottom:0;-webkit-transition: all 0.2s ease-in-out;-moz-transition: all 0.2s ease-in-out;-o-transition: all 0.2s ease-in-out;transition: all 0.2s ease-in-out;right: 0;margin: auto;}
.slideshow_heading.has-animated:after{width:34%;}
.slideshow_sub-title{margin-bottom:25px;opacity:0;}
.slideshow_sub-title p{font-size:24px;font-weight:400;line-height:normal;margin:0;text-transform:uppercase;}
.slideshow_btn{margin:0;background-color:#fff;color:#2a5b6c;font-size: 18px;padding: 0px 30px;letter-spacing: 0;font-weight: 300;height: 50px;line-height: 48px;box-shadow:8px 8px 8px rgba(0,0,0,0.3);-webkit-transition: all 0.2s ease-in-out;-moz-transition: all 0.2s ease-in-out;-o-transition: all 0.2s ease-in-out;transition: all 0.2s ease-in-out;opacity:0;}
.slideshow_btn:hover, .slideshow__heading-cta:focus{background-color:#f2a900;color:#fff;}
.slideshow_btn.btn:not(.disabled):hover::after, .slideshow__heading-cta.btn--secondary:not(.disabled):hover::after{outline:none;}
.slideshow_btn.btn::after, .slideshow_btn.btn--secondary::after{content:none;}
.guaranty-caption{background-image:url('yellow-bg.png');background-repeat:no-repeat;background-size: 100%;width:240px;height:240px;text-align:center;color: #fff;font-size: 24px;text-transform: uppercase;line-height: normal;font-weight: 500;padding:35px 25px 30px 40px;position:absolute;right:20px;bottom:30px;opacity:0;}
.guaranty-caption strong{font-weight:900;}
.caption-table{width:100%;display:table;height:100%;}
.caption-middle{display:table-cell;vertical-align:middle;padding-left:10px;position: relative;    left: -7px;}
.slick-active .slideshow_heading.has-animated{-webkit-animation: fadeUp 0.5s ease-out 0.5s 1 forwards;animation: fadeUp 0.5s ease-out 0.5s 1 forwards;}
.slick-active .slideshow_sub-title.has-animated{-webkit-animation: fadeUp 1s ease-out 1s 1 forwards;animation: fadeUp 1s ease-out 1s 1 forwards;}
.slick-active .slideshow_btn.has-animated{-webkit-animation: fadeUp 1.5s ease-out 1.5s 1 forwards;animation: fadeUp 1.5s ease-out 1.5s 1 forwards;}
.slick-active .guaranty-caption.has-animated{-webkit-animation: fadeUp 2s ease-out 2s 1 forwards;animation: fadeUp 2s ease-out 2s 1 forwards;}
